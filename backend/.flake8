[flake8]
max-line-length = 88
ignore = E203, E231, E501, W503, F403, E265, W293, E711, F601,F811,F541,F405
exclude =
    .git,
    __pycache__,
    migrations,
    venv,
    env,
    .env,
    .idea,
    .venv,
    ../frontend/*,
    ./frontend/*,
    ../bot/venv,
    ../bot/env,
    ../bot/.venv,
    ../bot/.env,
    ../bot/.idea/*,
    ../info-service/*,
    ../tg-frontend/*,
    ../web-frontend/*
    